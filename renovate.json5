{
  extends: [
    "config:base",
    "github>clivm/clivm-renovate-config#1.1.0",
    "github>clivm/clivm-renovate-config:installer-script#1.1.0(README\\.md)",
  ],
  automerge: true,
  major: {
    automerge: false,
  },
  regexManagers: [
    {
      fileMatch: ["README.md"],
      depNameTemplate: "clivm/clivm-installer",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "clivm-installer@(?<currentValue>.*)",
      ],
    },
    {
      fileMatch: ["README.md"],
      depNameTemplate: "clivm/clivm",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "clivm_version: (?<currentValue>.*?)\\n",
        "bash -s -- -v (?<currentValue>.*) -i",
      ],
    },
  ],
}
